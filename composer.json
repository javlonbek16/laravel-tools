{
    "name": "nematov/laravel-tools",
    "description": "Laravel package for generating and syncing repositories, controllers, interfaces and routes with customizable paths",
    "keywords": [
        "laravel",
        "package", 
        "repository",
        "controller",
        "interface",
        "generator"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/javlonbek16/laravel-tools",
    "authors": [
        {
            "name": "Javlonbek Nematov",
            "email": "nematovjalvonbek@gmail.com",
            "homepage": "https://github.com/javlonbek16"
        }
    ],
    "support": {
        "issues": "https://github.com/javlonbek16/laravel-tools/issues",
        "source": "https://github.com/javlonbek16/laravel-tools"
    },
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Src\\LaravelToolsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}