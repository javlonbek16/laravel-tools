{
    "name": "nematov/laravel-tools",
    "description": "Laravel package for generating and syncing repositories, controllers, interfaces and routes",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Src\\LaravelToolsServiceProvider"
            ]
        }
    }
}